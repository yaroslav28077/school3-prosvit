{
    "name": "school-prosvit",
    "version": "1.0.0",
    "description": "Website for Gymnasium Prosvit",
    "scripts": {
        "build": "eleventy && npx tailwindcss -i ./src/css/styles.css -o ./_site/css/styles.css --minify",
        "watch": "npx tailwindcss -i ./src/css/styles.css -o ./_site/css/styles.css --watch",
        "serve": "eleventy --serve",
        "dev": "npm-run-all --parallel serve watch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@11ty/eleventy": "^2.0.0",
        "@11ty/eleventy-navigation": "^0.3.5",
        "npm-run-all": "^4.1.5",
        "tailwindcss": "^3.3.0"
    }
}